<h2>Sub nav</h2>
{% if eleventyNavigation.parent == page.fileSlug | capitalize %}
    {% set find = page.fileSlug | capitalize %}
{% else %}
    {% set find = parentKey %}
{% endif %}
{% set pageTitle = page.fileSlug | capitalize %}

<pre>
<code>
{{ parentKey | dump(2) }}
{{ collections.pages | eleventyNavigation(parentKey) | dump(2) }}
{# {{collections.pages | eleventyNavigation(pageTitle) | dump(2)}} #}
</code>
</pre>

{% set levelOne = collections.pages | eleventyNavigation(parentKey) %}

<nav>
    <ul>
        {% for page in levelOne %}
            <li>
                {{ page | dump}}
                <a href="{{ page.url | url }}">
                    {{ page.title }}
                </a>
                <p>sub parent {{ page.parent }}</p>
                {% set levelTwo = page.children %}
                {# {% if levelTwo %} #}
                <ul>
                    {% for page in levelTwo %}
                        <li>
                            <a href="{{ page.url | url }}">
                                {{ page.title }}
                            </a>
                        </li>
                    {% endfor %}
                </ul>
                {# {% endif %} #}
            </li>
        {% endfor %}
    </ul>
</nav>