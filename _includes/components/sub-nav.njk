{% set navTitle = subMenu.navItems.navtitle %}
{% set subNav = subMenu.navItems %}
{% set pageIdentifier = page.fileSlug %}
<pre>
<code>
{{ subNav | dump(2)}}</code></pre>
{% for item in subNav %}
    {% set parent = item.parent_relation %}
    <p>parent: {{ parent }}</p>
    <p>pageIdentifier: {{ pageIdentifier }}</p>
    <p>page.url: {{ page.url }}</p>

    <pre>
<code>
{{item.navItem | dump(2)}}</code></pre>
    <nav>
        <ul>
            {% for link in item.navItem.link %}
                <li>
                    <a href="../{{ link.url | url }}"{% if link.url == page.url %} data-current="current item"{% endif %}>{{ link.displayText }}</a>
                </li>
            {% endfor %}
        </ul>
    </nav>
{% endfor %}